<simulation>
  <control>
    <duration>10</duration>
    <startmonth>1</startmonth>
    <startyear>2000</startyear>
  </control>

  <archetypes>
    <spec><path>stubs</path><lib>StubFacility</lib><archetype>StubFacility</archetype></spec>
    <spec><path>stubs</path><lib>StubRegion</lib><archetype>StubRegion</archetype></spec>
    <spec><path>stubs</path><lib>StubInst</lib><archetype>StubInst</archetype></spec>
  </archetypes>

  <facility>
    <name>OneFacility</name>
    <archetype>StubFacility</archetype>
    <agent>
      <StubFacility />
    </agent>
  </facility>

  <region>
    <name>OneRegion</name>
    <archetype>StubRegion</archetype>
    <agent>
      <StubRegion />
    </agent>
    <institution>
      <name>OneInst</name>
      <archetype>StubInst</archetype>
      <initialfacilitylist>
        <entry>
          <prototype>OneFacility</prototype>
          <number>1</number>
        </entry>
      </initialfacilitylist>
      <agent>
        <StubInst />
      </agent>
    </institution>
  </region>

</simulation>
