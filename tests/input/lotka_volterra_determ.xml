<!-- Lotka-Volterra simulation with Preys and Predators -->

<simulation>
  <control>
    <duration>20</duration>
    <startmonth>1</startmonth>
    <startyear>2000</startyear>
  </control>

  <archetypes>
    <spec><lib>agents</lib><archetype>Prey</archetype></spec>
    <spec><lib>agents</lib><archetype>Predator</archetype></spec>
    <spec><lib>agents</lib><archetype>NullRegion</archetype></spec>
    <spec><lib>agents</lib><archetype>NullInst</archetype></spec>
  </archetypes>

  <facility>
    <name>Prey</name>
    <archetype>Prey</archetype>
    <agent>
      <Prey>
        <birth_freq>1</birth_freq>
        <nchildren>1</nchildren>
        <birth_and_death>0</birth_and_death>
        <commod>wabbit</commod>
      </Prey>
    </agent>
  </facility>

  <facility>
    <name>Predator</name>
    <archetype>Predator</archetype>
    <agent>
      <Predator>
        <hunt_cap>2</hunt_cap>
        <hunt_freq>3</hunt_freq>
        <full>2</full>
        <lifespan>6</lifespan>
        <commod>wabbit</commod>
        <success>0.575</success>
        <birth_and_death>0</birth_and_death>
        <prey>Prey</prey>
      </Predator>
    </agent>
  </facility>

  <region>
    <name>SingleRegion</name>
    <archetype>NullRegion</archetype>
    <agent> <NullRegion/> </agent>
    <institution>
      <name>SingleInstitution</name>
      <archetype>NullInst</archetype>
      <initialfacilitylist>
        <entry>
          <prototype>Prey</prototype>
          <number>95</number>
        </entry>
        <entry>
          <prototype>Predator</prototype>
          <number>20</number>
        </entry>
      </initialfacilitylist>
      <agent> <NullInst/> </agent>
    </institution>
  </region>

  <!-- cyclus requires a recipe to not core dump...-->
  <recipe>
    <name>commod_recipe</name>
    <basis>mass</basis>
    <nuclide>
      <id>010010000</id>
      <comp>1</comp>
    </nuclide>
  </recipe>
</simulation>
