# Demonstrate Dakota script interface to Python script to interface to Cyclus

environment, 
	tabular_data 
		tabular_data_file = 'dakota_test.dat'

method,						
	multidim_parameter_study
    partitions = 1 

variables,					
	continuous_design = 1			
    lower_bounds      1500     
    upper_bounds      1600     
    descriptors       'x1'     

interface,							
	fork							
	  analysis_drivers = 'python cyclus_dakota.py'

responses,				
	num_objective_functions = 1	
	no_gradients
	no_hessians